<h1>New Question</h1>

<form action="<%=test_questions_path%>" method="post">
  <input name='authenticity_token' value='<%=form_authenticity_token %>' type='hidden'>

  <p>
    <label for="question-title">Title:</label>
    <input type=text name="question[title]" id="question-title">
  </p><br/>

  <p>
    <label for="question-test_id">Test id:</label>
    <input type='text' name="question[test_id]" id="question-test_id" value='<%= params[:test_id] %> ' >
  </p><br/>

  <input type='submit' value="Create">
</form>